pipeline{
    agent any
    environment {
        GIT_REP0 = "https://github.com/tejashree-dev/self_project.git"
        GIT_BRANCH = "master"
        DOKCER_IMAGE = "warkad/self_project"
        DOCKER_CREDENTIALS = "docker1"

    }
    stages{
        stage ('git clone repo'){
            steps{
                git credentialsId: 'github11', url: 'https://github.com/tejashree-dev/self_project.git'
            }
        }
        stage ('creat docker image'){
            steps{
                sh "docker build -t ${DOKCER_IMAGE} ."
            }
        }
        stage ('push docker image to dockerhub'){
            steps{
                script{
                    withDockerRegistry(credentialsId: 'docker1') {
                        sh "docker push ${DOCKER_CREDENTIALS}
                }

            }

        }
        stage ('ec2 deploying'){
            steps{

            }
        }
    }
    post{
        always{
            echo "pipeline has successfully completed."
        }
    }
}
